    "@telegram-apps/react-router-integration": "^1.0.1",
    "@telegram-apps/sdk-react": "^1.0.0",
    "@vapc/shared": "workspace:*",
    "@vapc/ui": "workspace:*",
    "bcryptjs": "^3.0.2",
    "blurhash": "^2.0.5",
    "bullmq": "^5.36.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "file-type": "^21.0.0",
    "ioredis": "^5.8.2",
    "jose": "^6.1.0",
    "lucide-react": "^0.552.0",
    "next": "^15.1.8",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-blurhash": "^0.3.0",
    "react-dom": "^19.0.0",
